    "dev": "npm-run-all --parallel watch server",
    "start": "node dist/index.js",
    "typeorm:cli": "ts-node ./node_modules/typeorm/cli.js -f src/ormconfig.ts",
    "migration:generate": "ts-node ./node_modules/typeorm/cli.js -f src/ormconfig.ts migration:generate -n",
    "migration:create": "ts-node ./node_modules/typeorm/cli.js -f src/ormconfig.ts migration:create -n",
    "migration:run": "ts-node ./node_modules/typeorm/cli.js -f src/ormconfig.ts migration:run"
  },
